{
  "name": "whois",
  "version": "1.1.5",
  "private": true,
  "description": "",
  "author": "John Kitsantas <proteus@devoidgr>",
  "license": "MIT",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc -p tsconfig.build.json",
    "clean": "rimraf dist",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "env-cmd nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:prod": "node dist/src/main.js",
    "start:docker": "node src/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "orm:sync": "ts-node ./node_modules/typeorm/cli.js schema:sync",
    "orm:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate",
    "orm:migrate": "ts-node ./node_modules/typeorm/cli.js migration:run"
  },
  "dependencies": {
    "@nestjs/common": "^6.10.1",
    "@nestjs/core": "^6.10.1",
    "@nestjs/platform-express": "^6.10.1",
    "@nestjs/swagger": "^3.1.0",
    "@nestjs/typeorm": "^6.2.0",
    "@nestjsx/crud": "^4.2.0",
    "@nestjsx/crud-typeorm": "^4.2.0",
    "axios": "^0.19.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "jsdom": "^15.2.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mysql2": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.3",
    "swagger-ui-express": "^4.1.2",
    "typeorm": "^0.2.20"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.10.1",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.23",
    "@types/jsdom": "^12.2.4",
    "@types/lodash": "^4.14.148",
    "@types/node": "^12.12.8",
    "@types/supertest": "^2.0.8",
    "env-cmd": "^10.0.1",
    "jest": "^24.9.0",
    "nodemon": "^1.19.4",
    "rimraf": "^3.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.5.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "5.20.1",
    "typescript": "^3.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
