{
  "name": "whois",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "author": "John Kitsantas <proteus@devoidgr>",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "orm:sync": "ts-node ./node_modules/typeorm/cli.js schema:sync",
    "orm:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate"
  },
  "dependencies": {
    "@nestjs/common": "^6.3.1",
    "@nestjs/core": "^6.2.0",
    "@nestjs/platform-express": "^6.2.0",
    "@nestjs/swagger": "^3.0.2",
    "@nestjs/typeorm": "^6.1.0",
    "@nestjsx/crud": "^3.2.0",
    "axios": "^0.18.1",
    "class-transformer": "^0.2.2",
    "class-validator": "^0.9.1",
    "jsdom": "^15.0.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "mysql": "^2.17.1",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.3",
    "rxjs": "^6.5.2",
    "swagger-ui-express": "^4.0.6",
    "typeorm": "^0.2.17"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.2.0",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.12",
    "@types/jsdom": "^12.2.3",
    "@types/lodash": "^4.14.126",
    "@types/node": "^12.0.0",
    "@types/supertest": "^2.0.7",
    "cross-env": "^5.2.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "tsconfig-paths": "^3.8.0",
    "tslint": "5.17.0",
    "typescript": "^3.5.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
